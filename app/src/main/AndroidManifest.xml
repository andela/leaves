<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.worldtreeinc.leaves" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name=".ParseApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <meta-data
            android:name="com.parse.APPLICATION_ID"
            android:value="@string/parse_id" />
        <meta-data
            android:name="com.parse.CLIENT_KEY"
            android:value="@string/parse_secret" />

        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GetStartedActivity"
            android:label="@string/title_activity_get_started" >
        </activity>
        <activity
            android:name=".WelcomeActivity"
            android:label="@string/welcome_activity_label" >
        </activity>
        <activity
            android:name=".LoginActivity"
            android:label="@string/title_activity_login" >
        </activity>
        <activity
            android:name=".RoleOptionActivity"
            android:label="@string/title_activity_role_option" >
        </activity>
        <activity
            android:name=".BidderDashActivity"
            android:label="@string/title_activity_bidder_dash" >
        </activity>
        <activity
            android:name=".PlannerDashActivity"
            android:label="@string/title_activity_planner_dash" >
        </activity>
    </application>

</manifest>
